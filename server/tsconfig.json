{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2019",
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"typeRoots": ["./types", "./node_modules/@types"]
	},
	"include": ["src/**/*", "types/**/*"],
	"exclude": [
		"node_modules",
		// "../tests/backend" 
	]
}


